<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="ring"
            android:thicknessRatio="13"
            android:useLevel="false">
            <solid android:color="@color/grafica_recorrido" />

        </shape>
    </item>
    <item>
        <rotate
            android:fromDegrees="90"
            android:toDegrees="90">
            <shape
                android:shape="ring"
                android:thicknessRatio="13"
                android:useLevel="true">
                <gradient
                    android:endColor="@color/grafica_end"
                    android:centerColor="@color/grafica_center"
                    android:startColor="@color/grafica_start"
                    android:type="sweep" />
            </shape>
        </rotate>
    </item>
    #002E2E2E
</layer-list>